* {
  margin: 0px;
  padding: 0px;
  font-family: Hiragino Sans GB, Arial, Helvetica, "Microsoft YaHei", sans-serif;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-appearance: none;
}
a {
  text-decoration: none;
}
ul,
li {
  list-style: none;
}
img {
  border: 0;
}
input,
select {
  outline: none;
}
.min-width {
  width:1000px;
  margin: 0 auto;
}
body {
  min-width:1000px;
}
.bg-img {
  background: url(../images/bg.png) no-repeat;
}
#header {

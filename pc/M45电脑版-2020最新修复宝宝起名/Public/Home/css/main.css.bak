  border: 0;
  width: 100%;
  font-size: 16px;
  color: #333;
  background: none;
}
.place-order ul li .input-box div.time {
  height: 36px;
  overflow: hidden;
  text-align: center;
  float: left;
  margin: 0 30px;
}
.place-order ul li .input-box div.time:first-child {
  margin-right: 30px;
}
.place-order ul li .input-box div.time select {
  font-size: 18px;
  border: 1px solid #a6a6a6;
  height: 34px;
